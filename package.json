    "@types/express": "^5.0.0",
    "@types/react": "~19.1.10",
    "babel-plugin-react-compiler": "^19.0.0-beta-e993439-20250117",
    "drizzle-kit": "^0.18.1",
    "eslint": "^9.31.0",
